{
  "compilerOptions": {
    "outDir": "./dist/",
    "sourceMap": true,
    "noImplicitAny": true,
    "module": "commonjs",
    "target": "es5",
    "jsx": "react",
    "baseUrl": ".",
    "paths": {
      "@view/*": [
        "./src/view/*"
      ],
      "@compoment/*": [
        "./src/compoment/*"
      ]
    }
  },
  "include": [
    "./src/**/*"
  ]
}